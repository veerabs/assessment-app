<?xml version='1.0' encoding='utf-8'?>
<widget id="express.assessment.app" version="1.0.3" versionCode="100011" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Assessment express</name>
    <description>
        An Assessment App
    </description>
    <author email="vpalsingh@deloitte.com" href="http://assessment.express">
        Assessment Express
    </author>
    <versionName>1.0.2</versionName>
    <content src="index.html" />
    <access origin="*" />
    <access origin="http://assessment.express" />
    <access origin="https://assessment.express" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="StatusBarStyle" value="default" />
    <preference name="phonegap-version" value="cli-6.1.0" />
</widget>
